<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <!-- 防止点击劫持；满足 CodeQL cs/web/missing-x-frame-options 规则 -->
        <add name="X-Frame-Options" value="DENY" />
        <!-- 现代浏览器的等价控制；与 X-Frame-Options 共同存在以覆盖更广 -->
        <add name="Content-Security-Policy" value="frame-ancestors 'none'" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>

