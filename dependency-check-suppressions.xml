<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
  <!-- Suppress known false-positives reported in CI logs -->
  <suppress>
    <notes>False positive: Maps Microsoft.ApplicationInsights.AspNetCore to aspnetcore 2.0 CVEs</notes>
    <packageUrl regex="true">^pkg:nuget/Microsoft\.ApplicationInsights\.AspNetCore@.*</packageUrl>
    <cve>CVE-2017-11883</cve>
  </suppress>
  <suppress>
    <packageUrl regex="true">^pkg:nuget/Microsoft\.ApplicationInsights\.AspNetCore@.*</packageUrl>
    <cve>CVE-2017-11770</cve>
  </suppress>
  <suppress>
    <notes>False positive: Maps Microsoft.AspNetCore.Authentication.JwtBearer to jwt_project:jwt</notes>
    <packageUrl regex="true">^pkg:nuget/Microsoft\.AspNetCore\.Authentication\.JwtBearer@.*</packageUrl>
    <cve>CVE-2025-45770</cve>
  </suppress>
  <suppress>
    <notes>False positive: Maps Microsoft.EntityFrameworkCore.SqlServer to www-sql_project</notes>
    <packageUrl regex="true">^pkg:nuget/Microsoft\.EntityFrameworkCore\.SqlServer@.*</packageUrl>
    <cve>CVE-2004-0455</cve>
  </suppress>
</suppressions>

